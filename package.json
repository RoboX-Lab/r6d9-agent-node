{
  "name": "r6d9-agent-node",
  "version": "0.1.0",
  "description": "A powerful Node.js library for browser automation and control powered by LLMs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "format": "prettier --write 'src/**/*.{ts,js,json,md}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testMatch '**/tests/**/*.unit.test.ts'",
    "test:integration": "jest --testMatch '**/tests/**/*.integration.test.ts'",
    "test:e2e": "jest --testMatch '**/tests/**/*.e2e.test.ts'",
    "example": "ts-node examples/basic-search-example.ts",
    "docs:generate": "typedoc --options typedoc.json",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "browser",
    "automation",
    "llm",
    "ai",
    "langchain",
    "playwright",
    "browser-automation",
    "dom-manipulation",
    "agent"
  ],
  "author": "R6D9 Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RoboX-Lab/r6d9-agent-node.git"
  },
  "bugs": {
    "url": "https://github.com/RoboX-Lab/r6d9-agent-node/issues"
  },
  "homepage": "https://github.com/RoboX-Lab/r6d9-agent-node#readme",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.50.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.25.12",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@langchain/community": "^0.3.30",
    "@langchain/core": "^0.3.40",
    "@langchain/langgraph": "^0.2.46",
    "@langchain/openai": "^0.4.4",
    "@solana/web3.js": "^1.87.3",
    "axios": "^1.7.9",
    "consola": "^3.4.0",
    "dotenv": "^16.4.7",
    "openai": "^4.85.1",
    "playwright": "^1.50.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ]
}